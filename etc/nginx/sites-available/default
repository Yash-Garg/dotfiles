server {
    listen 80;
    server_name localhost;
    index /_h5ai/public/index.php;
    root /home/ubuntu/Downloads;
    location / {
        try_files $uri $uri/ =404;
    }
    location ~ \.php$ {
        include snippets/fastcgi-php.conf;
        fastcgi_pass unix:/run/php/php-fpm.sock;
    }
}

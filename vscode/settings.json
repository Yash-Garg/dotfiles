{
  "workbench.iconTheme": "material-icon-theme",
  "explorer.confirmDelete": false,
  "workbench.startupEditor": "newUntitledFile",
  "editor.fontFamily": "'JetBrains Mono', monospace",
  "editor.fontWeight": "600",
  "editor.fontLigatures": "'calt', 'ss01'",
  "go.autocompleteUnimportedPackages": true,
  "python.languageServer": "Pylance",
  "remote.SSH.remotePlatform": {
    "skybox": "linux"
  },
  "editor.suggestSelection": "first",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.activityBar.visible": true,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: LLVM, UseTab: Never, IndentWidth: 4, TabWidth: 4, BreakBeforeBraces: Attach, AllowShortIfStatementsOnASingleLine: false, IndentCaseLabels: false, ColumnLimit: 0, AccessModifierOffset: -4 }",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "dart.flutterSdkPath": "E:\\Android\\Flutter",
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "workbench.productIconTheme": "fluent-icons",
  "files.trimTrailingWhitespace": true,
  "editor.formatOnSave": true,
  "go.toolsManagement.autoUpdate": true,
  "dart.debugSdkLibraries": false,
  "debug.console.fontSize": 13,
  "settingsSync.ignoredExtensions": [
    "eyhn.vscode-vibrancy"
  ],
  "dart.showInspectorNotificationsForWidgetErrors": false,
  "markdown.preview.typographer": true,
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "editor.cursorSmoothCaretAnimation": true,
  "dart.warnWhenEditingFilesOutsideWorkspace": false,
  "editor.linkedEditing": true,
  "editor.suggest.preview": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
  "dart.warnWhenEditingFilesInPubCache": false,
  "editor.inlineSuggest.enabled": true,
  "editor.scrollbar.verticalScrollbarSize": 10,
  "editor.scrollbar.horizontalScrollbarSize": 10,
  "python.defaultInterpreterPath": "C:\\Users\\Yashg\\AppData\\Local\\Programs\\Python\\Python310\\python.exe",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "terminal.integrated.fontWeightBold": "normal",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.inlayHints.fontFamily": "'JetBrains Mono', monospace",
  "diffEditor.codeLens": true,
  "editor.inlayHints.fontSize": 14,
  "workbench.colorCustomizations": {
    "editorLineNumber.activeForeground": "#faf8f8"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "keyword",
          "constant",
          "storage.modifier",
          "storage.type.class"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      }
    ],
    "[Horizon Italic]": {
      "functions": "#00c3ffe7",
      "keywords": "#d400ffe7",
      "types": "#ffbb00ee",
      "variables": "#ff5151"
    },
    "[Community Material Theme Darker High Contrast]": {
      "keywords": "#ff5959",
      "types": "#fcce78",
      "functions": "#74b0ff"
    }
  },
  "[rust]": {
    "editor.defaultFormatter": "statiolake.vscode-rustfmt"
  },
  "terminal.integrated.fontSize": 15,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.colorTheme": "One Dark Pro Darker",
  "C_Cpp.autocompleteAddParentheses": true,
  "C_Cpp.clang_format_style": "{ BasedOnStyle: LLVM, UseTab: Never, IndentWidth: 4, TabWidth: 4, BreakBeforeBraces: Attach, AllowShortIfStatementsOnASingleLine: false, IndentCaseLabels: false, ColumnLimit: 0, AccessModifierOffset: -4 }",
  "livePreview.notifyOnOpenLooseFile": false,
  "editor.renderControlCharacters": true,
  "dart.flutterSdkPaths": [
    "E:/Android/fvm/versions"
  ],
  "bloc.checkForUpdates": false,
  "editor.minimap.enabled": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.fontSize": 16,
  "editor.wordWrap": "on",
  "workbench.editor.enablePreview": false,
  "editor.foldingImportsByDefault": true,
  "dart.checkForSdkUpdates": false,
  "editor.hideCursorInOverviewRuler": true,
  "C_Cpp.default.cppStandard": "c++17",
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "git.confirmSync": false,
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.cursorStyle": "block",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.currentLine.enabled": true,
  "cmake.configureOnOpen": true,
  "workbench.editor.closeOnFileDelete": true,
  "explorer.confirmDragAndDrop": false,
  "workbench.editor.historyBasedLanguageDetection": true,
  "dart.debugExternalPackageLibraries": true
}

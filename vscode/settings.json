{
    "explorer.confirmDelete": false,
    "workbench.startupEditor": "newUntitledFile",
    "editor.fontFamily": "'JetBrains Mono', 'Cascadia Code', monospace",
    "editor.fontWeight": "bold",
    "editor.fontLigatures": "'calt' on, 'liga' on, 'zero' on",
    "editor.suggestSelection": "first",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false,
        "editor.defaultFormatter": "Dart-Code.dart-code"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.activityBar.visible": true,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "dart.flutterSdkPath": "E:\\Android\\Flutter",
    "dart.previewFlutterUiGuides": true,
    "dart.previewFlutterUiGuidesCustomTracking": true,
    "workbench.productIconTheme": "fluent-icons",
    "files.trimTrailingWhitespace": true,
    "editor.formatOnSave": true,
    "dart.debugSdkLibraries": false,
    "debug.console.fontSize": 13,
    "settingsSync.ignoredExtensions": [
        "eyhn.vscode-vibrancy"
    ],
    "dart.showInspectorNotificationsForWidgetErrors": false,
    "markdown.preview.typographer": true,
    "terminal.integrated.fontWeight": "600",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "editor.cursorSmoothCaretAnimation": "on",
    "dart.warnWhenEditingFilesOutsideWorkspace": false,
    "editor.linkedEditing": true,
    "editor.suggest.preview": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
    "dart.warnWhenEditingFilesInPubCache": false,
    "editor.inlineSuggest.enabled": true,
    "editor.scrollbar.verticalScrollbarSize": 10,
    "editor.scrollbar.horizontalScrollbarSize": 10,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "terminal.integrated.fontWeightBold": "600",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.inlayHints.fontFamily": "'JetBrains Mono', 'Cascadia Code', monospace",
    "editor.inlayHints.padding": true,
    "diffEditor.codeLens": true,
    "editor.inlayHints.fontSize": 16,
    "workbench.colorCustomizations": {
        "editor.foldBackground": "#37374ece",
        "editorInlayHint.background": "#252538",
        "editorInlayHint.typeBackground": "#252538",
        "editorInlayHint.parameterBackground": "#252538"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "livePreview.notifyOnOpenLooseFile": false,
    "editor.renderControlCharacters": true,
    "dart.flutterSdkPaths": [
        "E:/Android/fvm/versions"
    ],
    "bloc.checkForUpdates": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "editor.fontSize": 16,
    "editor.wordWrap": "on",
    "workbench.editor.enablePreview": false,
    "editor.foldingImportsByDefault": true,
    "dart.checkForSdkUpdates": false,
    "editor.hideCursorInOverviewRuler": true,
    "git.confirmSync": false,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.cursorStyle": "block",
    "terminal.integrated.cursorBlinking": true,
    "workbench.editor.closeOnFileDelete": true,
    "explorer.confirmDragAndDrop": false,
    "workbench.editor.historyBasedLanguageDetection": true,
    "dart.debugExternalPackageLibraries": true,
    "redhat.telemetry.enabled": true,
    "workbench.iconTheme": "material-icon-theme",
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "javascript.inlayHints.enumMemberValues.enabled": true,
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
    "javascript.inlayHints.parameterTypes.enabled": true,
    "typescript.inlayHints.enumMemberValues.enabled": true,
    "javascript.inlayHints.variableTypes.enabled": true,
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
    "typescript.inlayHints.variableTypes.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": true,
        "dart": true
    },
    "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font', 'Cascadia Code', monospace",
    "remote.SSH.remotePlatform": {
        "angerycat": "linux",
        "codebin": "linux"
    },
    "editor.accessibilitySupport": "off",
    "workbench.colorTheme": "Catppuccin Mocha",
    "[nix]": {},
    "terminal.integrated.fontSize": 16,
    "git.openRepositoryInParentFolders": "never",
    "extensions.ignoreRecommendations": true,
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "terminal.integrated.gpuAcceleration": "auto",
    "terminal.integrated.customGlyphs": true,
    "C_Cpp.autocompleteAddParentheses": true,
    "C_Cpp.clang_format_style": "{ BasedOnStyle: LLVM, UseTab: Never, IndentWidth: 4, TabWidth: 4, BreakBeforeBraces: Attach, AllowShortIfStatementsOnASingleLine: false, IndentCaseLabels: false, ColumnLimit: 0, AccessModifierOffset: -4 }",
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.useCtrlKeys": true,
    "vim.useSystemClipboard": true,
    "vim.handleKeys": {
        "<tab>": false,
        "<C-d>": true,
        "<C-a>": false,
        "<C-s>": false,
        "<C-v>": false,
        "<C-x>": false,
        "<C-c>": false,
        "<C-y>": false,
        "<C-f>": false,
        "<C-z>": false
    },
    "direnv.restart.automatic": true,
    "direnv.status.showChangesCount": false
}